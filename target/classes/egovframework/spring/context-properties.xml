<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean name="propertiesService" class="egovframework.rte.fdl.property.impl.EgovPropertyServiceImpl" destroy-method="destroy">
		<property name="properties">
			<map>
				<entry key="pageUnit" value="10" />
				<entry key="pageSize" value="10" />
				<entry key="Naver.Email" value="${Naver.Email}" />
				<entry key="Mail.TmpPwdTitle" value="${Mail.TmpPwdTitle}" />
				<entry key="Mail.TmpPwdContent" value="${Mail.TmpPwdContent}" />
				<entry key="Mail.IdSearchTitle" value="${Mail.IdSearchTitle}" />
				<entry key="Mail.IdSearchContent" value="${Mail.IdSearchContent}" />
				<entry key="Mail.SurveyCompletTitle" value="${Mail.SurveyCompletTitle}" />
				<entry key="Mail.SurveyCompletContent" value="${Mail.SurveyCompletContent}" />
			</map>
		</property>
	</bean>
</beans>	
